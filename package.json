{
  "name": "appwrite-orm",
  "version": "0.1.0",
  "description": "A TypeScript ORM package for Appwrite with web and server support",
  "main": "dist/index.js",
  "readme": "https://appwrite-orm.readthedocs.io",
  "homepage": "https://appwrite-orm.readthedocs.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/raisfeld-ori/appwrite-orm"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./web": {
      "import": "./dist/web/index.js",
      "require": "./dist/web/index.js",
      "types": "./dist/web/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "publish": "npm publish --access public && release-it",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy"
  },
  "keywords": [
    "appwrite",
    "ORM",
    "orm",
    "database",
    "typescript"
  ],
  "author": "Ori raisfeld",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "release-it": "19.0.6"
  },
  "dependencies": {
    "appwrite": ">=13.0.0"
  },
  "peerDependencies": {
    "appwrite": ">=13.0.0"
  }
}
